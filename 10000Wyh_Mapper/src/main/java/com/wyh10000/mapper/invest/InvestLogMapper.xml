<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyh10000.mapper.invest.InvestLogMapper">
  <resultMap id="BaseResultMap" type="com.wyh10000.domain.invest.InvestLog">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="iid" jdbcType="INTEGER" property="iid" />
    <result column="uid" jdbcType="INTEGER" property="uid" />
    <result column="money" jdbcType="INTEGER" property="money" />
    <result column="content" jdbcType="VARCHAR" property="content" />
    <result column="createtime" jdbcType="TIMESTAMP" property="createtime" />
  </resultMap>
  <sql id="Base_Column_List">
    id, iid, uid, money, content, createtime
  </sql>
  <select id="selectByIid" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_investlog
    where iid = #{iid}
  </select>
  <select id="selectNew" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_investlog order by createtime desc limit 0,10
  </select>
  <insert id="insert" parameterType="com.wyh10000.domain.invest.InvestLog">
    insert into t_investlog (iid, uid,  money, content, createtime)
    values ( #{iid,jdbcType=INTEGER}, #{uid,jdbcType=INTEGER}, #{money,jdbcType=INTEGER}, #{content,jdbcType=VARCHAR},now())
  </insert>

</mapper>