<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--1、应用名称设置-->
    <dubbo:application name="investprovider"></dubbo:application>
    <!--2、配置注册中心-->
    <dubbo:registry address="10.8.163.50:2181" protocol="zookeeper" check="false"></dubbo:registry>
    <!--3、设置协议-->
    <dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>
    <!--4、设置发布的服务-->
    <dubbo:service interface="com.wyh10000.service.invest.InvestService" ref="investProvider" retries="0"></dubbo:service>
    <dubbo:service interface="com.wyh10000.service.invest.IncrementService" ref="incrementProvider" retries="0"></dubbo:service>

</beans>